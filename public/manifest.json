{
    "manifest_version": 3,
    "name": "New tab",
    "description": "Display bookmark directories on new tab page",
    "version": "0.1.0",
    "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": ["bookmarks", "storage", "favicon"],
    "web_accessible_resources": [
        {
            "resources": ["_favicon/*"],
            "matches": ["<all_urls>"],
            "extension_ids": ["*"]
        }
    ]
}
