{
  "manifest_version": 2,

  "name": "New tab",
  "description": "New tab extension",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "chrome://favicon/"
  ]
}